<!DOCTYPE html>
<html>
<head>
	<title>User List</title>
</head>
<body>
	<h1>User List</h1>

	<a href="/home">Back</a> | 
	<a href="/logout">logout</a> 
	<br>

	<table border="1">
		<tr>
			<td>ID</td>
			<td>Emplyoee Name</td>
			<td>Contact Number</td>
			<td>User Name</td>
			<td>Password</td>
		</tr>

		<% 
			users.forEach( function(emp){ %>
			<tr>
				<td><%= emp.id %></td>
				<td><%= emp.ename %></td>
				<td><%= emp.conum %></td>
				<td><%= emp.uname %></td>
				<td><%= emp.password %></td>
				<td>
					<a href="/user/edit/<%= emp.id %>">Edit</a> |
				</td>
			</tr>

		<%
			}); 
		%>
		
	</table>

</body>
</html>